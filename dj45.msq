CREATE TABLE xmeta (trackid INTEGER NOT NULL, p INTEGER DEFAULT 4, e INTEGER DEFAULT 4, n INTEGER DEFAULT 4, c INTEGER DEFAULT 4, i INTEGER DEFAULT 4, l INTEGER DEFAULT 4, PRIMARY KEY (trackid));
CREATE TABLE "cddbmeta" (trackid INTEGER NOT NULL, file VARCHAR(256) NOT NULL, title VARCHAR(128) DEFAULT 'Missing Title' NOT NULL, length INTEGER NOT NULL, album VARCHAR(128), artist VARCHAR(64), year NUMERIC DEFAULT 1900, tracknum INTEGER DEFAULT 1, path TEXT(128), PRIMARY KEY (trackid), UNIQUE (file));
